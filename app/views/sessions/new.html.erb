<h1><%= t('.title') %></h1>

<%= form_with model: @user, url: session_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.submit t('.sign_in') %>
  </div>
<% end %>
